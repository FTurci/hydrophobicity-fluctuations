  orthogonal box = (-20.729646 -20.729646 -10) to (20.729646 20.729646 30)
  orthogonal box = (-20.729646 -20.729646 -20.729646) to (20.729646 20.729646 20.729646)
reset_timestep	0

pair_style	sw
pair_coeff	* * mW.sw mW
Reading sw potential file mW.sw with DATE: 2007-06-11


mass		1 ${mwMass}
mass		1 18.015

neighbor	2 bin #Angstroms
neigh_modify	every 1 delay 4
# zero the momentum
velocity	all zero linear

timestep 5.0
fix		1 all nvt temp ${T} ${T} 500
fix		1 all nvt temp 426 ${T} 500
fix		1 all nvt temp 426 426 500

# dump mydump all atom 1000 dump.nvt.rho${desiredRho}.T${T}.atom
thermo		100
thermo_style custom step temp pe etotal press vol density

variable csvname string output.nvt.rho${desiredRho}.T${T}.csv
variable csvname string output.nvt.rho0.9.T${T}.csv
variable csvname string output.nvt.rho0.9.T426.csv
fix extra all print 100 "$(step),$(pe),$(temp),$(press),$(density),$(vol)" file ${csvname} screen no title "step,pe,temperature,pressure,density,volume"
fix extra all print 100 "$(step),$(pe),$(temp),$(press),$(density),$(vol)" file output.nvt.rho0.9.T426.csv screen no title "step,pe,temperature,pressure,density,volume"


run		${runsteps}
run		50000000
