  orthogonal box = (-10 -10 -10) to (30 30 30)
  2 by 1 by 3 MPI processor grid
  2144 atoms
  replicate CPU = 0.001 seconds
# calculate half of the box size in armstrongs for a cubic box
variable        num_atoms equal count(all)
variable        hL     equal  (${num_atoms}/0.602214076*${mwMass}/${desiredRho})^(1./3.)*0.5
variable        hL     equal  (2144/0.602214076*${mwMass}/${desiredRho})^(1./3.)*0.5
variable        hL     equal  (2144/0.602214076*18.015/${desiredRho})^(1./3.)*0.5
variable        hL     equal  (2144/0.602214076*18.015/0.9)^(1./3.)*0.5
print        ":: The   "
